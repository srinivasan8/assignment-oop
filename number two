 +getvolume(double):double
  def __str__(self):
    return "Radius: "+ str(self.radius) + ", Height: " + str(self.height)
 
  def getRadius(self):
    return self.radius
  def getHeight(self):
    return self.height
  def getVolume(self):
      return self.radius*self.radius*3.14*self.height
 

